# Telegram bot token from @BotFather
BOT_TOKEN=123456789:REPLACE_ME

# Local dev example:
# DATABASE_URL=postgresql+asyncpg://expense:expense@localhost:5432/expense
#
# Docker compose example (postgres service name is "postgres"):
DATABASE_URL=postgresql+asyncpg://expense:expense@postgres:5432/expense

LOG_LEVEL=INFO
SQL_ECHO=false
DASHBOARD_DEBOUNCE_SECONDS=2.0

# docker-compose postgres settings
POSTGRES_DB=expense
POSTGRES_USER=expense
POSTGRES_PASSWORD=expense
